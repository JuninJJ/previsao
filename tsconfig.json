{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "paths": { "@/*": ["./*"] },
    "noEmit": true
  }
}
